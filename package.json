{
  "name": "cxchord",
  "version": "1.0.2",
  "description": "Midi Chord Recognizer",
  "main": "lib/CxChord.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "chart.js": "^2.1.3",
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "electron": "^1.4.1",
    "jasmine": "^2.5.3",
    "uglifyjs": "latest",
    "watch": "latest"
  },
  "scripts": {
    "start": "electron .",
    "clean": "rimraf lib/CxChord*",
    "build:tsc": "tsc -p ./ --declaration -t es5 --outFile lib/CxChord.js --module system",
    "build:test": "tsc -p ./test/spec/ --declaration -t es5 --outFile lib/CxChordSpec.js --module system",
    "build": "npm run clean && npm run build:tsc && npm run build:test",
    "predist": "npm run build",
    "dist": "uglifyjs lib/CxChord.js  > lib/CxChord.min.js"
  },
  "keywords": [
    "Midi",
    "Chords",
    "Recognition"
  ],
  "author": "Claus Fjelloe-Jensen <claus.fjello@gmail.com>",
  "license": "ISC"
}
