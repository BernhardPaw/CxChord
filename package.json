{
  "name": "cxchord",
  "version": "1.0.2",
  "description": "Midi Chord Recognizer",
  "main": "lib/CxChord.js",
  "typings": "lib/CxChord",
  "directories": {
    "src": "src",
    "build": "build",
    "example": "example",
    "declarations": "declare",
    "test": "test",
    "lib": "lib"
  },
  "dependencies": {
    "@types/node": "^7.0.13",
    "chart.js": "^2.1.3",
    "columnify": "^1.5.4",
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "electron": "^1.4.1",
    "jasmine": "^2.5.3",
    "uglifyjs": "latest",
    "watch": "latest"
  },
  "scripts": {
    "start": "electron .",
    "clean": "rimraf src/*.js && rimraf src/*.js.map && rimraf lib/CxChord* && rimraf declare/src/*.d.ts  && rimraf declare/test/spec/*.d.ts && rimraf build/*.js && rimraf build/*.js.map",
    "build:tsc": "tsc -p ./",
    "build": "npm run clean && npm run build:tsc",
    "predist": "npm run build",
    "dist": "jake libJS && jake typings && uglifyjs lib/CxChord.js  > lib/CxChord.min.js"
  },
  "keywords": [
    "Midi",
    "Chords",
    "Recognition"
  ],
  "author": "Claus Fjelloe-Jensen <claus.fjello@gmail.com>",
  "license": "ISC"
}
